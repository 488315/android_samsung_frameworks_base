<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.graphics.drawable.SeslRecoilDrawable app:seslRecoilColor="@color/qs_edit_button_ripple_color"
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="@color/qs_edit_ripple_bg_color" />
            <stroke android:width="1.0dip" android:color="#0affffff" />
            <corners android:radius="@dimen/qs_setting_edit_button_ripple_radius" />
        </shape>
    </item>
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white" />
            <corners android:radius="@dimen/qs_setting_edit_button_ripple_radius" />
        </shape>
    </item>
</androidx.appcompat.graphics.drawable.SeslRecoilDrawable>
