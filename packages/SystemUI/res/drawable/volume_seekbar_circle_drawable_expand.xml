<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/volume_seekbar_radius" />
            <solid android:color="@color/volume_seekbar_background_color_expand" />
            <stroke android:width="@dimen/sub_full_volume_seekbar_stroke_width_expand" android:color="@color/volume_seekbar_background_stroke_color_expand" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <com.android.systemui.volume.view.SecRoundedCornerSeekBarDrawable android:drawable="@drawable/volume_seekbar_progress_drawable" />
    </item>
</layer-list>
