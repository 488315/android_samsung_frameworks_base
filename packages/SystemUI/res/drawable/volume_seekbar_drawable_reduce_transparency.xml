<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <size android:height="@dimen/volume_seekbar_width" />
            <corners android:radius="@dimen/volume_seekbar_radius" />
            <solid android:color="@color/transparent" />
            <stroke android:width="1.0dip" android:color="@color/volume_seekbar_background_stroke_color_reduce_transparency" />
        </shape>
    </item>
    <item android:gravity="fill_horizontal|center" android:id="@android:id/progress">
        <com.android.systemui.volume.view.SecRoundedCornerSeekBarDrawable android:drawable="@drawable/volume_seekbar_progress_drawable" />
    </item>
</layer-list>
