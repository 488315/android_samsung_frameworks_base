<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.graphics.drawable.SeslRecoilDrawable app:seslRecoilColor="@color/sec_qs_tiles_ripple_background"
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item>
        <androidx.appcompat.util.SeslShapeDrawable android:shape="rectangle">
            <solid android:color="@color/qs_tile_coloring_container_bg" />
            <stroke android:width="1.0dip" android:color="@color/qs_tile_container_bg_border_color" />
            <corners android:radius="@dimen/panel_bar_corner_radius" />
        </androidx.appcompat.util.SeslShapeDrawable>
        <stroke android:width="1.0dip" android:color="#26ffffff" />
    </item>
    <item android:id="@android:id/mask">
        <androidx.appcompat.util.SeslShapeDrawable android:shape="rectangle">
            <corners android:radius="@dimen/panel_bar_corner_radius" />
            <stroke android:width="1.0dip" android:color="@color/qs_tile_container_bg_border_color" />
            <solid android:color="@android:color/white" />
        </androidx.appcompat.util.SeslShapeDrawable>
    </item>
</androidx.appcompat.graphics.drawable.SeslRecoilDrawable>
