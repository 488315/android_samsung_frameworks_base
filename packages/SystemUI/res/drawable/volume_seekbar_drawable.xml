<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <size android:height="@dimen/volume_seekbar_width" />
            <corners android:radius="@dimen/volume_seekbar_radius" />
            <solid android:color="@color/transparent" />
            <stroke android:width="@dimen/volume_seekbar_stroke_width" android:color="@color/volume_seekbar_background_stroke_color" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <size android:height="@dimen/volume_seekbar_width" />
                <corners android:radius="@dimen/volume_seekbar_radius" />
                <solid android:color="@color/volume_seekbar_progress_color" />
                <stroke android:width="@dimen/volume_seekbar_stroke_width" android:color="@color/volume_seekbar_progress_color" />
            </shape>
        </clip>
    </item>
</layer-list>
